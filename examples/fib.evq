/* computes the fibonacci sequence up until n */

: fib
  0 swap 1 swap               /* n -> a b n = 0 1 n */
  begin
    dup 0 >                   /* while n > 0 ... */
  while
    rot rot                   /* a b n -> n a b */
    tuck +                    /* n a b -> n b a+b */
    rot                       /* n b a+b -> b a+b n */
    1 -                       /* decrement n */
  repeat
  drop drop ;                 /* drop n and b, leave a = F(n) */

10 fib .  -> 55